{
  "name": "amfi_bot_frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p lint type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview --port 80",
    "full-check": "run-p type-check prettier lint",
    "full-check-fix": "run-p type-check prettier-fix lint-fix",
    "test:unit": "vitest --environment jsdom",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint-fix": "run lint --fix",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write ."
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "discord-api-types": "^0.37.49",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "query-parse": "^2.0.0",
    "sass": "^1.63.6",
    "tippy.js": "^6.3.7",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "vite": "^4.4.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0-beta.14-77e850b",
    "vue-router": "^4.2.4",
    "vue-tippy": "^6.2.0",
    "vue3-click-away": "^1.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/js-cookie": "^3.0.3",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.19",
    "@types/nprogress": "^0.2.0",
    "@types/uuid": "^9.0.2",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.5"
  },
  "packageManager": "yarn@3.6.0"
}
